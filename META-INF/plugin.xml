<idea-plugin version="2">
  <id>com.guidewire.build.ijdevkitmvn</id>
  <name>Intellij plugin development with Maven</name>
  <description>Intellij plugin to support plugin development with Maven</description>
  <version>${project.version}</version>

  <vendor email="buildmaster-dev@guidewire.com.com">Guidewire Software</vendor>
  <idea-version since-build="117.105"/>

  <depends>org.jetbrains.idea.maven</depends>
  <depends>DevKit</depends>

  <extensions defaultExtensionNs="org.jetbrains.idea.maven">
    <importer implementation="com.guidewire.build.ijdevkitmvn.ijplugin.PluginModuleImporter"/>
  </extensions>
</idea-plugin>
